	</modules>

	<parent>
		<groupId>org.springframework.boot</groupId>
		<artifactId>spring-boot-starter-parent</artifactId>
		<version>1.5.9.RELEASE</version>
		<relativePath /> <!-- lookup parent from repository -->
	</parent>

	<properties>
		<hibernate.version>4.3.11.Final</hibernate.version>
		<project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
		<java.version>1.8</java.version>
		<common.util.version>1.1.0-RELEASE</common.util.version>
		<de.codecentric.version>1.5.2</de.codecentric.version>
		<jsonrpc4j.version>1.2.0</jsonrpc4j.version>
		<portlet.api.version>2.0</portlet.api.version>
		<jwt.version>3.3.0</jwt.version>
		<rocketmq.version>3.6.2.Final</rocketmq.version>
		<ons.client.version>1.7.9.Final</ons.client.version>
		<hikaricp.version>3.3.1</hikaricp.version>

		<nemo.core.api.version>1.9.0-RELEASE</nemo.core.api.version>
		<nemo.core.version>1.10.10-RELEASE</nemo.core.version>
		<nemo.dao.api.version>1.11.1-RELEASE</nemo.dao.api.version>
		<nemo.dao.core.version>1.10.7-RELEASE</nemo.dao.core.version>
		<nemo.dao.hibernate.version>1.11.1-RELEASE</nemo.dao.hibernate.version>
		<nemo.mq.core.api.version>1.9.7-RELEASE</nemo.mq.core.api.version>
		<nemo.mq.core.version>1.10.6-RELEASE</nemo.mq.core.version>
		<nemo.mq.activemq.version>1.10.6-RELEASE</nemo.mq.activemq.version>
		<nemo.mq.rabbitmq.version>1.10.6-RELEASE</nemo.mq.rabbitmq.version>
		<nemo.mq.rocketmq.version>1.10.9-RELEASE</nemo.mq.rocketmq.version>
		<nemo.rpc.eureka.api.version>1.9.1-RELEASE</nemo.rpc.eureka.api.version>
		<nemo.rpc.eureka.version>1.10.10-RELEASE</nemo.rpc.eureka.version>
		<nemo.lock.version>1.9.7-RELEASE</nemo.lock.version>
		<nemo.security.version>1.10.13-RELEASE</nemo.security.version>
		<nemo.fuse.version>1.10.4-RELEASE</nemo.fuse.version>
		<nemo.monitor.server.version>1.10.11-RELEASE</nemo.monitor.server.version>
		<nemo.rpc.eureka.server.version>1.10.11-RELEASE</nemo.rpc.eureka.server.version>
		<nemo.config.server.version>1.4.0-RELEASE</nemo.config.server.version>
		<nemo.sliding.window.version>1.10.3-RELEASE</nemo.sliding.window.version>
		<nemo.id.generator.version>1.9.21-RELEASE</nemo.id.generator.version>
		
	</properties>

	<profiles>
		<profile>
			<id>test</id>
			<build>
				<finalName>ROOT</finalName>
			</build>
			<properties>
				<build.packaging>war</build.packaging>
				<build.tomcat.scope>provided</build.tomcat.scope>
			</properties>
		</profile>
		<profile>
			<id>default</id>
			<properties>
				<build.packaging>jar</build.packaging>
				<build.tomcat.scope>compile</build.tomcat.scope>
			</properties>
			<activation>
				<activeByDefault>true</activeByDefault>
			</activation>
		</profile>
	</profiles>

	<dependencyManagement>
		<dependencies>
			<dependency>
				<groupId>com.jimistore.util</groupId>
				<artifactId>common-util-parent</artifactId>
				<version>${common.util.version}</version>
				<type>pom</type>
				<scope>import</scope>
			</dependency>
			<dependency>
				<groupId>org.springframework.cloud</groupId>
				<artifactId>spring-cloud-dependencies</artifactId>
				<version>Dalston.SR1</version>
				<type>pom</type>
				<scope>import</scope>
			</dependency>

			<dependency>
				<groupId>com.jimistore.boot</groupId>
				<artifactId>nemo-core-api</artifactId>
				<version>${nemo.core.api.version}</version>
			</dependency>

