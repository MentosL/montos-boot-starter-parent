			</dependency>
			<dependency>
				<groupId>com.jimistore.boot</groupId>
				<artifactId>nemo-mq-core</artifactId>
				<version>${nemo.mq.core.version}</version>
			</dependency>
			<dependency>
				<groupId>com.jimistore.boot</groupId>
